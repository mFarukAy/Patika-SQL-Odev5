ANSWER 1: SELECT * FROM film
          WHERE title ~~ '%n'
          ORDER BY length DESC
          limit 5;

ANSWER 2: SELECT * FROM film
          WHERE title ~~ '%n'
          ORDER BY length ASC
          OFFSET 5
          LIMIT 5;
          
ANSWER 3: SELECT * FROM customer
          WHERE store_id = 1
          ORDER BY last_name DESC
          LIMIT 4          
